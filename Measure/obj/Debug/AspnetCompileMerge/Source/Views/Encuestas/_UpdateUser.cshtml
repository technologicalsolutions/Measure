@using Measure.ViewModels.Usuario

@model ViewUpdateUser
<div class="col-md-6 col-12">
    <div class="row mt-1">
        <div class="col-12">
            <strong>@Measure.Recursos.Recurso.Nombres</strong>
        </div>
        <div class="col-12">
            <div class="row">
                <div class="col-12 mt-2">
                    @Html.TextBoxFor(m => m.Nombres, new { @class = "form-control update" })
                </div>
            </div>
        </div>
    </div>
</div>
